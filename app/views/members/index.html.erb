<p id="notice"><%= notice %></p>
<p id="alert"><%= alert %></p>

<h1>Các câu hỏi</h1>

<table class="table">
  <thead>
    <tr>
      <th>Câu hỏi</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Thêm câu hỏi', new_member_path %>
